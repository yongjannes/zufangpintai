<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zufang.lease.web.app.mapper.GraphInfoMapper">
    <select id="selectListByItemTypeAndId" resultType="com.zufang.lease.model.entity.GraphInfo">
    SELECT * FROM graph_info WHERE item_type = #{itemType} AND id = #{id}
</select>
</mapper>
